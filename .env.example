# AI Symptom Checker Environment Configuration
# Copy this file to .env.local and replace with your actual values

# OpenRouter AI API Configuration
# Get your API key from: https://openrouter.ai/
VITE_OPENROUTER_API_KEY=sk-or-v1-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Firebase Configuration (optional - app works in demo mode without these)
# Get these from Firebase Console: https://console.firebase.google.com/
VITE_FIREBASE_API_KEY=AIzaSyXxXxXxXxXxXxXxXxXxXxXxXxXxXxXxXxX
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:xxxxxxxxxxxxxxxxxxxxxxxx

# Optional Configuration Flags
# Set to 'true' to disable Firebase entirely (app will work in offline mode)
VITE_DISABLE_FIREBASE=false

# Set to 'true' to force demo mode even with valid API keys
VITE_USE_DEMO_MODE=false
